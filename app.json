{
  "expo": {
    "name": "GameTime",
    "slug": "gametime",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/GameTimeLogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/GameTimeLogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dpuender.gametime"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/GameTimeLogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/digital-7 (mono).ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c55fb97a-ef21-456f-9179-bf0f00fbb51a"
      }
    },
    "owner": "dpuender"
  }
}
